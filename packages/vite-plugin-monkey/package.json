{
  "name": "vite-plugin-monkey",
  "version": "3.2.2",
  "description": "A vite plugin server and build your.user.js for userscript engine like Tampermonkey and Violentmonkey and Greasemonkey",
  "main": "dist/node/index.cjs",
  "module": "dist/node/index.mjs",
  "types": "dist/node/index.d.ts",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/node/index.mjs",
      "require": "./dist/node/index.cjs",
      "types": "./dist/node/index.d.ts"
    },
    "./dist/client": {
      "import": "./dist/client/index.mjs",
      "types": "./dist/client/index.d.ts"
    },
    "./dist/native": {
      "import": "./dist/native/index.mjs",
      "types": "./dist/native/index.d.ts"
    },
    "./client": {
      "types": "./client.d.ts"
    },
    "./global": {
      "types": "./global.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "global.d.ts",
    "client.d.ts"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsup && tsx ./scripts/transform_type.ts",
    "build:watch": "tsup --watch"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "monkey",
    "Tampermonkey",
    "Violentmonkey",
    "Greasemonkey",
    "userscript",
    "greasyfork",
    "typescript"
  ],
  "author": "lisonge",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lisonge/vite-plugin-monkey.git",
    "directory": "packages/vite-plugin-monkey"
  },
  "bugs": {
    "url": "https://github.com/lisonge/vite-plugin-monkey/issues"
  },
  "homepage": "https://github.com/lisonge/vite-plugin-monkey#readme",
  "dependencies": {
    "acorn-walk": "^8.2.0",
    "cross-spawn": "^7.0.3",
    "detect-port": "^1.5.1",
    "htmlparser2": "^9.0.0",
    "magic-string": "^0.30.1",
    "mrmime": "^1.0.1",
    "open": "^9.1.0",
    "picocolors": "^1.0.0",
    "systemjs": "^6.14.1"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/detect-port": "^1.3.3"
  },
  "peerDependencies": {
    "vite": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "engines": {
    "pnpm": ">=6",
    "node": "^14.18 || >= 16"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
